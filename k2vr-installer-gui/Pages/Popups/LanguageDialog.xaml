        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:res = "clr-namespace:k2vr_installer_gui.Properties;assembly=k2vr-installer-gui"
        xmlns:local="clr-namespace:k2vr_installer_gui.Pages.Popups"
        mc:Ignorable="d"
        Title="{x:Static res:Resources.home_language_button}" Height="162.81" Width="472.727" ShowInTaskbar="False" Topmost="True" ResizeMode="NoResize" Icon="/k2vr-installer-gui;component/installer-icon.ico" WindowStartupLocation="CenterScreen">

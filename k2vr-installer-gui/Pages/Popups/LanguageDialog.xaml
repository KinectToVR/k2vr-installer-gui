        <Button Style="{StaticResource Button_Primary}" x:Name="lang_ok" FontFamily="Segoe MDL2 Assets" Content="" FontSize="28" HorizontalAlignment="Left" Height="60" Margin="395,66,0,0" VerticalAlignment="Top" Width="60"/>
        <Button Style="{StaticResource Button_Primary}" x:Name="lang_cancel" FontFamily="Segoe MDL2 Assets" Content="" FontSize="28" HorizontalAlignment="Left" Height="60" Margin="327,66,0,0" VerticalAlignment="Top" Width="60"/>

    </Grid>
